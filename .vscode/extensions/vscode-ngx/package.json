{
  "name": "vscode-ngx",
  "private": true,
  "version": "0.0.0",
  "main": "./ext.js",
  "contributes": {
    "grammars": [
      {
        "language": "typescriptngx",
        "scopeName": "source.ts.ngx",
        "path": "./syntaxes/angularx.tmLanguage.json"
      },
      {
        "path": "./syntaxes/injection.tmLanguage.json",
        "scopeName": "ngx-template.injection",
        "injectTo": ["source.ngx2"]
      }
    ],
    "languages": [
      {
        "id": "typescriptngx",
        "aliases": [
          "typescriptngx"
        ],
        "extensions": [
          ".ng.tsx",
          ".ngx"
        ],
        "configuration": "./language-configuration.json"
      }
    ]
  },
  "engines": {
    "vscode": "^1.22.0"
  }
}
